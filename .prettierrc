{
	"semi": true,
	"singleQuote": true,
	"parser": "typescript",
	"tabWidth": 4,
	"printWidth": 100,
	"useTabs": true,
	"bracketSpacing": true,
	"overrides": [
		{
			"files": [".prettierrc", ".eslintrc", "package.json", "*.json"],
			"options": {
				"parser": "json"
			}
		},
		{
			"files": ["*.html"],
			"options": {
				"parser": "html"
			}
		}
	]
}
